!function(){        require.async(["wiki-common:widget/lib/jquery/jquery","wiki-lemma:widget/basicElement/modulePager/modulePager"],function($,Pager){
           var pager=new Pager({
                total:2,
                handleNext:function(index){
                    $(".musicSingle-wrapper").find(".page-wrapper").hide();
                    $(".musicSingle-wrapper").find(".pager-"+index).show();
                },
                handlePrev:function(index){
                    $(".musicSingle-wrapper").find(".page-wrapper").hide();
                    $(".musicSingle-wrapper").find(".pager-"+index).show();
                }
           });
           pager.appendTo($(".musicSingle-pager"));
        });
    }();
!function(){        require.async(["wiki-common:widget/lib/jquery/jquery","wiki-lemma:widget/basicElement/modulePager/modulePager"],function($,Pager){
           var pager=new Pager({
                total:1,
                handleNext:function(index){
                    $(".musicOther-wrapper").find(".page-wrapper").hide();
                    $(".musicOther-wrapper").find(".pager-"+index).show();
                },
                handlePrev:function(index){
                    $(".musicOther-wrapper").find(".page-wrapper").hide();
                    $(".musicOther-wrapper").find(".pager-"+index).show();
                }
           });
           pager.appendTo($(".musicOther-pager"));
        });
    }();
!function(){	        require.async(["wiki-common:widget/lib/jquery/jquery","wiki-lemma:widget/basicElement/modulePager/modulePager"],function($,Pager){
	            var slideTo=function(index){
	                var scroller=$(".magazine");
	                scroller.stop().animate({scrollLeft:(index-1)*790},200);
	            };
	            var pager=new Pager({
	                total:6,
	                handleNext:function(index){
	                    slideTo(index);
	                },
	                handlePrev:function(index){
	                    slideTo(index);
	                }
	            });
	            pager.appendTo($(".magazine-pager"));
	        });
	    }();
