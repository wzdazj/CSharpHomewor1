function logImg(src){
    var img = new Image()
    img.onerror = img.onload = function(){
        img = null
    };
    img.src = src;
}

function jinImgEvnet(){
    var img = document.getElementById('jinImg');
    if(img){
      logImg("https://nsclick.baidu.com/v.gif?name=jinImgShow&pid=111&tt="+(new Date()).getTime())
    }

    var jinImgHerf = document.getElementById('jinImgHerf');
    jinImgHerf.onclick = function(){
      logImg("https://nsclick.baidu.com/v.gif?name=jinImgHerfClick&pid=111&tt="+(new Date()).getTime())
    }

    document.getElementById('jinClose').onclick = function(e){
      if (document.all) {
        event.returnValue = false;
      } else {
       e.preventDefault();
      };
      document.getElementById('jinAdvWrapper').style.display = 'none';
      logImg("https://nsclick.baidu.com/v.gif?name=jinImgClose&pid=111&tt="+(new Date()).getTime())
    }
}

